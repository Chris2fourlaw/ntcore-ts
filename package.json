    "@nrwl/linter": "15.4.4",
    "@nrwl/nx-cloud": "^15.0.2",
    "@nrwl/workspace": "15.4.4",
    "@types/jest": "28.1.1",
    "@types/node": "16.11.7",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.36.1",
