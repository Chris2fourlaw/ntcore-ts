    "lint-staged": "^13.1.0",
    "nx": "15.4.4",
    "prettier": "^2.6.2",
    "ts-jest": "28.0.5",
    "ts-node": "10.9.1",
    "typedoc": "^0.23.23",
    "typedoc-plugin-missing-exports": "^1.0.0",
    "typescript": "~4.8.2"
