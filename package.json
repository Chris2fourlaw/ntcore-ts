{
  "name": "ntcore-ts",
  "version": "0.0.1-alpha.0",
  "description": "A TypeScript Client for WPILib's NetworkTables v4.0 protocol",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepublish": "npm run build && npm run test",
    "build": "tsc --build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "format": "npm run prettier:fix && npm run lint:fix",
    "test": "jest",
    "prepare": "husky install && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Chris2fourlaw/ntcore-ts.git"
  },
  "keywords": ["networktables", "ntcore", "wpilib"],
  "author": "Chris Lawson",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Chris2fourlaw/ntcore-ts/issues"
  },
  "homepage": "https://github.com/Chris2fourlaw/ntcore-ts#readme",
  "lint-staged": {
    "*.{js,ts,json}": ["npm run format"]
  },
  "dependencies": {
    "@msgpack/msgpack": "^2.8.0",
    "tslib": "^2.4.1",
    "uuid": "^9.0.0",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@types/eslint": "^8.4.10",
    "@types/jest": "^29.2.5",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-standard-with-typescript": "^26.0.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.2.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-n": "^15.6.0",
    "eslint-plugin-no-relative-import-paths": "^1.5.2",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-tsdoc": "^0.2.17",
    "husky": "^8.0.3",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "lint-staged": "^13.1.0",
    "np": "^7.6.3",
    "prettier": "^2.8.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
}
