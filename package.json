    "eslint-plugin-tsdoc": "^0.2.17",
    "husky": "^9.1.7",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-websocket-mock": "^2.5.0",
    "lint-staged": "^15.3.0",
